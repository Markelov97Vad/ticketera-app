{
  "name": "store",
  "version": "1.0.0",
  "scripts": {
    "check": "prettier --check './src/**/*.{ts,tsx}'",
    "format": "prettier --write './src/**/*.{ts,tsx}'",
    "lint": "stylelint './src/**/*.module.scss'",
    "lintfix": "stylelint './src/**/*.module.scss' --fix",
    "fixAll": "npm run format && npm run lintfix",
    "build": "webpack --mode production",
    "start": "webpack serve --env mode=development port=3001 ",
    "build:dev": "webpack --env mode=development",
    "build:prod": "webpack --env mode=production",
    "build:analyzer": "npm run build:prod --env analyzer=true",
    "build:desktop": "webpack --env mode=production --env platform=desktop",
    "build:mobile": "webpack --env mode=production --env platform=mobile",
    "build:typescript": "webpack --env mode=production --env platform=desktop --env typeChecking=true"
  },
  "dependencies": {
    "@packages/build-config": "*",
    "@packages/shared": "*",
    "@reduxjs/toolkit": "^2.0.1",
    "@types/react-redux": "^7.1.33",
    "@types/react-router-dom": "^5.3.3",
    "moment": "^2.29.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^9.1.0",
    "react-router-dom": "^6.20.1"
  },
  "devDependencies": {
    "@babel/core": "^7.23.5",
    "@babel/preset-react": "^7.23.3",
    "@babel/preset-typescript": "^7.23.3",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.11",
    "@svgr/webpack": "^8.1.0",
    "@types/babel__core": "^7.20.5",
    "@types/node": "^20.8.3",
    "@types/react": "^18.2.42",
    "@types/react-dom": "^18.2.17",
    "@types/webpack": "^5.28.3",
    "@types/webpack-bundle-analyzer": "^4.6.3",
    "@types/webpack-dev-server": "^4.7.2",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.0",
    "babel-loader": "^9.1.3",
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.8.1",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.2",
    "eslint-plugin-react": "^7.33.2",
    "fork-ts-checker-webpack-plugin": "^9.0.2",
    "html-webpack-plugin": "^5.5.3",
    "mini-css-extract-plugin": "^2.7.6",
    "react-refresh": "^0.14.0",
    "react-refresh-typescript": "^2.0.9",
    "sass": "^1.69.5",
    "sass-loader": "^13.3.2",
    "style-loader": "^3.3.3",
    "stylelint": "^16.1.0",
    "stylelint-config-standard-scss": "^13.0.0",
    "stylelint-scss": "^6.0.0",
    "ts-loader": "^9.5.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2",
    "typescript-plugin-css-modules": "^5.0.2",
    "webpack": "^5.89.0",
    "webpack-bundle-analyzer": "^4.10.1",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1"
  }
}
